{
  "packageManager": "yarn@3.8.2",
  "name": "svgo-ll",
  "version": "6.2.0-alpha.1",
  "description": "svgo-ll is a Node.js library and command-line application for optimizing SVG images.",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/svg-utils/svgo-ll#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/svg-utils/svgo-ll"
  },
  "main": "./lib/svgo.js",
  "bin": "./bin/svgo.js",
  "types": "./lib/svgo-node.d.ts",
  "exports": {
    ".": {
      "import": "./lib/svgo.js",
      "require": "./dist/svgo-node.cjs",
      "types": "./lib/svgo.d.ts"
    },
    "./browser": {
      "import": "./dist/svgo.browser.js",
      "types": "./lib/svgo.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/svgo-node.d.ts"
      ],
      "browser": [
        "./lib/svgo.d.ts"
      ]
    }
  },
  "files": [
    "bin",
    "lib",
    "plugins",
    "dist",
    "!**/*.test.js"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "node scripts/sync-version.js && rollup -c",
    "typecheck": "tsc",
    "lint": "eslint . && prettier --check .",
    "fix": "eslint --fix . && prettier --write .",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=4 --coverage",
    "test:bundles": "yarn build && node ./test/svgo.cjs && node ./test/browser.js",
    "test:regress": "cross-env NO_DIFF=1 node ./test/regression.js -i ./test/fixtures/files && cross-env NO_DIFF=1 node ./test/regression.js --enable round -i ./test/fixtures/files",
    "qa": "yarn typecheck && yarn lint && yarn test && yarn test:bundles && yarn test:regress",
    "prepublishOnly": "rimraf dist && yarn build"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "fixtures"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "commander": "^14.0.1",
    "css-select": "^6.0.0",
    "css-tree": "^3.1.0",
    "csso": "^5.0.5",
    "picocolors": "^1.1.1",
    "sax": "^1.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/csso": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/sax": "^1.2.7",
    "@types/tar-stream": "^3.1.4",
    "buffer": "^6.0.3",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.55.1",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.2",
    "string_decoder": "^1.3.0",
    "tar-stream": "^3.1.7",
    "typescript": "^5.9.3"
  }
}
